@import 'nib'
//see https://github.com/visionmedia/nib/tree/master/lib/nib

@import 'mymixins'


html
	background #000
	color #FFF
	
body
	font 12px Helvetica, 'lucida grande', tahoma, verdana, arial, sans-serif
	margin 0
	padding 0
	background linear-gradient(top, BEIGE, $softBeige)
	min-width 760px
	width 100%
	min-height 100%
	color #FFF

h1,h2,h3,h4
	padding 0
	margin 0
	font-weight bold
